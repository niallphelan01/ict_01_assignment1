<table class="ui table">
    <thead>

    <tr>
        <th>Date</th>
        <th>Weight</th>
        <th>Chest</th>
        <th>Thigh</th>
        <th>upper arm</th>
        <th>waist</th>
        <th>hips</th>
        <th>Comment</th>
    </tr>
    </thead>
    <tbody>
    {{#each assessmentList}}
        <tr>
            <td>
                {{date}}
            </td>
            <td>
                {{weight}}
            </td>
            <td>
                {{chest}}
            </td>
            <td>
                {{thigh}}
            </td>
            <td>
                {{upperarm}}
            </td>
            <td>
                {{waist}}
            </td>
            <td>
                {{hips}}
            </td>
            <td>
                {{comment}}
            </td>
            <td>
                <a href="/editAssessment/{{id}}" class="ui icon button">
                    <i class="upload icon"></i>
                </a>
            </td>
        </tr>
    {{/each}}
    </tbody>
</table>