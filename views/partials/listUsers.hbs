<table class="ui table">
    <thead>
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th></th>
    </tr>
    </thead>
    <tbody>
    {{#each userlist}}
        <tr>
            <td>
                {{firstName}}
            </td>
            <td>
                {{lastName}}
            </td>
            <td>
                {{email}}
            </td>
        <td>
            <a href="/assessmentList/{{id}}/" class="ui icon button">
                <i class="icon folder open"></i>
            </a>
        </td>
            <td>
                <a href="/deleteUser/{{id}}/" class="ui icon button">
                    <i class="icon trash"></i>
                </a>
            </td>
        </tr>
    {{/each}}
    </tbody>
</table>